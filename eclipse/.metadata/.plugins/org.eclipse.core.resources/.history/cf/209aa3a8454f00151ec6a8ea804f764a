package com.magiology.objhelper;

import java.util.HashMap;
import java.util.Map;

public class ConvenientMap{
	
	public Map<String, Integer> ints=new HashMap<String, Integer>();
	public Map<String, Float> floats=new HashMap<String, Float>();
	public Map<String, Boolean> booleans=new HashMap<String, Boolean>();
	public Map<String, Double> doubles=new HashMap<String, Double>();
	
	
}
