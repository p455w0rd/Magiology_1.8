package com.magiology.core.init;

import com.magiology.forgepowered.packets.skeleton.AbstractToClientMessage;
import com.magiology.forgepowered.packets.skeleton.AbstractToServerMessage;
import com.magiology.forgepowered.packets.toclient.SavableDataWithKeyPacket;
import com.magiology.forgepowered.packets.toclient.SendPlayerDataPacket;
import com.magiology.forgepowered.packets.toserver.ClickHologramPacket;
import com.magiology.forgepowered.packets.toserver.NotifyPointedBoxChangePacket;
import com.magiology.forgepowered.packets.toserver.OpenGuiPacket;
import com.magiology.forgepowered.packets.toserver.RenderObjectUploadPacket;
import com.magiology.forgepowered.packets.toserver.RightClickBlockPacket;
import com.magiology.forgepowered.packets.toserver.TileRedstone;
import com.magiology.forgepowered.packets.toserver.UploadPlayerDataPacket;
import com.magiology.forgepowered.packets.toserver.generic.GenericServerIntPacket;
import com.magiology.forgepowered.packets.toserver.generic.GenericServerStringPacket;
import com.magiology.forgepowered.packets.toserver.generic.GenericServerVoidPacket;

public class MPackets{

	public static void preInit(){
		AbstractToServerMessage.registerNewMessage(RightClickBlockPacket.class);
		AbstractToServerMessage.registerNewMessage(TileRedstone.class);
		AbstractToServerMessage.registerNewMessage(OpenGuiPacket.class);
		AbstractToServerMessage.registerNewMessage(GenericServerIntPacket.class);
		AbstractToServerMessage.registerNewMessage(GenericServerVoidPacket.class);
		AbstractToServerMessage.registerNewMessage(GenericServerStringPacket.class);
		AbstractToServerMessage.registerNewMessage(UploadPlayerDataPacket.class);
		AbstractToServerMessage.registerNewMessage(ClickHologramPacket.class);
		AbstractToServerMessage.registerNewMessage(RenderObjectUploadPacket.class);
		AbstractToServerMessage.registerNewMessage(NotifyPointedBoxChangePacket.class);
		AbstractToServerMessage.registerNewMessage(NotifyPointedBoxChangePacket.class);
		
		AbstractToClientMessage.registerNewMessage(SendPlayerDataPacket.class);
		AbstractToClientMessage.registerNewMessage(SavableDataWithKeyPacket.class);
		
		
	}

}
