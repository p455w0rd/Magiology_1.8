		super(new GuiHologramProjectorMainContainer(player,tile));
		this.player=player;
		this.tile=tile;
	}
		

	@Override
	protected void drawGuiContainerBackgroundLayer(float partialTicks, int mouseX, int mouseY){

	}

}
