package com.magiology.modedmcstuff.gui.chatevents;

import net.minecraft.entity.player.EntityPlayer;
import net.minecraft.util.ChatComponentText;

public class ClickEvent_UPDATE_MOD extends AbstractCustomClickEvent{
	public ClickEvent_UPDATE_MOD(Action theAction, String theValue,EntityPlayer player){
		super(theAction, theValue, player);
	}
	@Override
	protected void onClickEvent(){
		player.addChatMessage(new ChatComponentText("BOOOM!!!"));
	}
}
