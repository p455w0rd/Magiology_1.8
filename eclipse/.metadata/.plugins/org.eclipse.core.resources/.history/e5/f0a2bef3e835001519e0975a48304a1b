package com.magiology.gui.guiContainer;

import net.minecraft.entity.player.EntityPlayer;
import net.minecraft.entity.player.InventoryPlayer;
import net.minecraft.inventory.Container;

import com.magiology.api.power.skeleton.TileEntityPow;

public class GuiHologramProjectorContainer extends Container{
	
	private final TileEntityPow tileCB;
	
	public GuiHologramProjectorContainer(InventoryPlayer pInventory,TileEntityPow tileCB2){
		tileCB=tileCB2;
	}
	
	
	
	@Override
	public boolean canInteractWith(EntityPlayer player){
		return true;
	}
	@Override
	public void onContainerClosed(EntityPlayer p_75134_1_){
		 super.onContainerClosed(p_75134_1_);
	}
}
